<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper.AnnouncementMapper">
    <resultMap id="AnnouncementResultMapper" type="Announcement">
        <result column="_date" property="date"></result>
    </resultMap>

    <select id="selectAll" resultType="com.pojo.Announcement">
        select *
        from announcement;
    </select>

</mapper>