<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper.AvatarMapper">


    <select id="selectByUsername" resultType="com.pojo.Avatar">
        select *
        from avatar where username=#{username};
    </select>
</mapper>